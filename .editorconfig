[*]
charset = utf-8
end_of_line = lf
insert_final_newline = false
indent_style = space
indent_size = 4

[{composer.lock, .stylelintrc, .babelrc, .eslintrc, jest.config, *.bowerrc, *.jsb3, *.jsb2, *.json}]
indent_style = space
indent_size = 2

[{blazar-manifest.json, custom-manifest.json, *.json5}]
indent_style = space
indent_size = 2

[{*.sql, *.ddl, *.cql}]
indent_style = space
indent_size = 2

[{*.yml, *.yaml}]
indent_style = space
indent_size = 2

